    "react-router-dom": "^6.4.2",
    "redux": "^4.2.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.19.1",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
